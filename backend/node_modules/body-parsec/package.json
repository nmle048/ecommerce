    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "devDependencies": {
    "@tinyhttp/app": "^0.2.62",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/koa": "^2.11.4",
    "@types/node": "^14.6.2",
    "@types/supertest": "^2.0.10",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "koa": "^2.12.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "tsup": "^3.6.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup src/*.ts --format cjs,esm --dts --minify-whitespace",
    "prepare": "pnpm build && pnpm test"
  },
  "optionalDependencies": {
    "express": "^4.17.1",
    "koa": "^2.12.0"
